<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <form action="/" method="post">
      <input type="submit">
    </form>

    <% orders.forEach(order => { %>
      <li><%= order._id %>, <%= order.created_at %></li>
    <% }) %>
  </body>
</html>
